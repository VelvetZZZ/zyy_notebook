{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Scheme file",
      "type": "process",
      "command": "csi",
      "args": [
        "-script",
        "${file}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ]
}